
{
  "name": "prodvor-monorepo",
  "private": true,
  "type": "module",
  "packageManager": "pnpm@10.13.1",
  "scripts": {
    "install:all": "pnpm install --frozen-lockfile",
    "dev": "pnpm --filter prodvor-frontend dev",
    "build": "pnpm --filter \"./**\" build",
    "lint": "pnpm --filter \"./**\" lint",
    "typecheck": "pnpm --filter \"./**\" typecheck",
    "start:backend": "pnpm --filter prodvor-backend start:prod",
    "start:frontend": "pnpm --filter prodvor-frontend start"
  },
  "devDependencies": {
    "@nestjs/cli": "^10.4.2",
    "@nestjs/schematics": "^10.1.3",
    "@nestjs/testing": "^10.3.10",
    "@tailwindcss/typography": "^0.5.13",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.12",
    "@types/lodash.debounce": "^4.0.8",
    "@types/multer": "^1.4.11",
    "@types/node": "^20.14.12",
    "@types/passport-jwt": "^4.0.1",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "@types/supertest": "^6.0.2",
    "autoprefixer": "^10.4.19",
    "eslint-config-next": "14.2.5",
    "jest": "^29.7.0",
    "postcss": "^8.4.40",
    "prisma": "^5.17.0",
    "source-map-support": "^0.5.21",
    "supertest": "^7.0.0",
    "tailwindcss": "^3.4.7",
    "ts-jest": "^29.2.3",
    "ts-loader": "^9.5.1",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.5.4"
  }
}
