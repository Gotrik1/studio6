{
  "name": "prodvor-monorepo",
  "private": true,
  "workspaces": [
    "frontend",
    "backend"
  ],
  "scripts": {
    "dev:frontend": "npm run dev --workspace=frontend",
    "dev:backend": "npm run start:dev --workspace=prodvor-backend",
    "build:frontend": "npm run build --workspace=frontend",
    "build:backend": "npm run build --workspace=prodvor-backend",
    "lint": "npm run lint --workspace=frontend && npm run lint --workspace=prodvor-backend"
  }
}
